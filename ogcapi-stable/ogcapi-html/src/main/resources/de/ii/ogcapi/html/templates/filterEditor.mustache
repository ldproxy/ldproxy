{{#filterEditor}}
<script type="text/javascript">
  globalThis._filter = {
      container: 'filter',
      language: '{{{language}}}',
      translationsDe: {
        {{#translationsMapDe}}
          '{{key}}': '{{{value}}}',
        {{/translationsMapDe}}
      },
      translationsEn: {
      {{#translationsMapEn}}
        '{{key}}': '{{{value}}}',
      {{/translationsMapEn}}
      },
      {{#temporalExtent}}
      temporal: {
          start: {{#start}}{{.}}{{/start}}{{^start}}0{{/start}},
          end: {{#end}}{{.}}{{/end}}{{^end}}Date.now(){{/end}}
      },
    {{/temporalExtent}}
    {{#bbox}}
      spatial: [[{{minLng}},{{minLat}}],[{{maxLng}},{{maxLat}}]],
    {{/bbox}}
    {{#backgroundUrl}}
      backgroundUrl: '{{{.}}}',
    {{/backgroundUrl}}
    {{#attribution}}
      attribution: '{{{.}}}',
    {{/attribution}}
  }
</script>

{{> app-filtereditor}}

{{/filterEditor}}

